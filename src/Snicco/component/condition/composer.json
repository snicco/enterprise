{
  "name": "snicco/condition",
  "description": "Condition classes for WordPress",
  "license": "proprietary",
  "authors": [
    {
      "name": "Calvin Alkan",
      "email": "calvin@snicco.de"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "snicco/str-arr": "^1.2.1"
  },
  "require-dev": {
    "codeception/codeception": "4.2",
    "lucatume/wp-browser": "3.1.6"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Snicco\\Enterprise\\Component\\Condition\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Snicco\\Enterprise\\Component\\Condition\\Tests\\": "tests"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    }
  },
  "extra": {
    "docker-services": [
      "app",
      "wp",
      "db"
    ]
  },
  "scripts": {
    "test": [
      "vendor/bin/codecept run"
    ]
  }
}
