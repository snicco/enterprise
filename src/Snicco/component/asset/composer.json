{
    "name": "snicco/asset",
    "description": "An object oriented API to use assets generated by a bundler.",
    "license": "proprietary",
    "authors": [
        {
            "name": "Calvin Alkan",
            "email": "calvin@snicco.de"
        }
    ],
    "require": {
        "ext-json": "*"
    },
    "require-dev": {
        "codeception/codeception": "4.2"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Snicco\\Enterprise\\Component\\Asset\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Snicco\\Enterprise\\Component\\Asset\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/codecept run"
        ]
    }
}
